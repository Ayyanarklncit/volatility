[DEFAULT]
profile = WinXPSP2x86
active = True
filename = xp-laptop-2005-06-25.img

[TestMutantScan]
--profile = %(profile)s
commandline = symlinkscan
--filename = %(filename)s
active = True

[TestMemmap]
--profile = %(profile)s
commandline = memmap --pid=2624
--filename = %(filename)s
active = True

[TestConsoles]
--profile = %(profile)s
commandline = consoles
--filename = %(filename)s
active = True

[TestVad]
--profile = %(profile)s
--filename = %(filename)s
active = True
ng_commandline = vad --pid 2624

[TestConnections]
--profile = %(profile)s
commandline = connections
--filename = %(filename)s
active = True

[TestPSScan]
--profile = %(profile)s
commandline = psscan
--filename = %(filename)s
active = True

[TestDLLList]
trunk_replace_regex = s/\|\|\n/||-\n/
--filename = %(filename)s
commandline = dlllist
--profile = %(profile)s
active = True

[TestConnectionScanners]
--profile = %(profile)s
commandline = connscan
--filename = %(filename)s
active = True

[TestPrintkey]
--profile = %(profile)s
commandline = printkey
--filename = %(filename)s
active = True

[TestModules]
--profile = %(profile)s
commandline = modules
--filename = %(filename)s
active = True

[TestPSXview]
--profile = %(profile)s
commandline = psxview
--filename = %(filename)s
active = True

[TestVadInfo]
--profile = %(profile)s
commandline = vadinfo --pid 2624
--filename = %(filename)s
active = True

[TestPFN]
--profile = %(profile)s
test_data = { "0x81faf280": ["0x320010", "0x7ffdf043"],
	"0x81ed84e8":  ["0x240200", "0x360012"]
	}
--filename = %(filename)s
active = True

[TestSockets]
--profile = %(profile)s
commandline = sockets
--filename = %(filename)s
active = True

[TestRegDump]
--profile = %(profile)s
--filename = %(filename)s
active = True
ng_commandline = regdump --dump-dir %(tempdir)s

[TestDriverIrp]
--profile = %(profile)s
commandline = driverirp
--filename = %(filename)s
active = True

[TestDeviceTree]
trunk_replace_regex = 
	s/ [ -]+ / /
	s/ +/ /
ng_replace_regex = 
	s/UNKNOWN \(\d+\)/UNKNOWN/
	s/ [ -]+ / /
	s/ +/ /
--filename = %(filename)s
commandline = devicetree
--profile = %(profile)s
active = True

[TestDLLDump]
--filename = %(filename)s
commandline = dlldump --pid 4012 --dump-dir %(tempdir)s
pid = 4012
active = True
--profile = %(profile)s

[TestHandles]
--profile = %(profile)s
commandline = handles --pid 1484
--filename = %(filename)s
active = True

[TestYara]
--filename = %(filename)s
trunk_commandline = yarascan -Y sys --kernel
active = True
ng_commandline = yarascan --string sys
--profile = %(profile)s

[TestDriverScan]
--profile = %(profile)s
commandline = driverscan
--filename = %(filename)s
active = True

[TestPS]
--profile = %(profile)s
commandline = pslist
--filename = %(filename)s
active = True

[TestSymlinkScan]
--profile = %(profile)s
commandline = symlinkscan
--filename = %(filename)s
active = True

[TestCommandScan]
--profile = %(profile)s
--filename = %(filename)s
active = True
ng_commandline = cmdscan

[TestMalFind]
--profile = %(profile)s
active = True
commandline = malfind --pid 840
--filename = %(filename)s
ng_replace_regex = s/\*+\n/\n/

[TestSvcScan]
--profile = %(profile)s
commandline = svcscan
--filename = %(filename)s
active = True

[TestKDBGScan]
--profile = %(profile)s
commandline = kdbgscan
--filename = %(filename)s
active = True

[TestMemmapCoalesce]
--profile = %(profile)s
--filename = %(filename)s
active = True
ng_commandline = memmap --pid=2624 --coalesce

[TestCallBacks]
--profile = %(profile)s
commandline = callbacks
--filename = %(filename)s
active = True

[TestProcdump]
--filename = %(filename)s
trunk_commandline = procexedump --pid 2536 --dump-dir %(tempdir)s
pid = 2536
active = True
ng_commandline = procdump --pid 2536 --dump-dir %(tempdir)s
--profile = %(profile)s

[TestImpScan]
--filename = %(filename)s
commandline = impscan --pid 4012
pid = 4012
active = True
--profile = %(profile)s

